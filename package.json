{
  "name": "alias-gandi-angular",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload --proxy-config proxy.config.json",
    "dev": "netlify dev",
    "prod": "ng serve --live-reload --prod --proxy-config proxy.config.json",
    "build": "ng build --prod && npm ci --prefix netlify && npm run lambda:tsc && npm run nlb && ./cleanLambdas.sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && netlify deploy",
    "nlb": "netlify functions:build --src netlify/lambdas",
    "lambda:tsc": "npm run build --prefix netlify",
    "lambda:tsc:watch": "npm start --prefix netlify",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.4",
    "@angular/cdk": "^10.2.3",
    "@angular/common": "~10.1.4",
    "@angular/compiler": "~10.1.4",
    "@angular/core": "~10.1.4",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.1.4",
    "@angular/material": "^10.2.3",
    "@angular/platform-browser": "~10.1.4",
    "@angular/platform-browser-dynamic": "~10.1.4",
    "@angular/router": "~10.1.4",
    "@angular/service-worker": "~10.1.4",
    "netlify-cli": "^2.40.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2",
    "@angular/cli": "~10.1.4",
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular/compiler-cli": "~10.1.4",
    "@angular/language-service": "~10.1.4",
    "typescript": "~4.0.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0"
  },
  "devDependencies": {}
}
