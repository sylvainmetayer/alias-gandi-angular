{
  "name": "alias-gandi-angular",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload --proxy-config proxy.config.json",
    "dev": "netlify dev",
    "prod": "ng serve --live-reload --prod --proxy-config proxy.config.json",
    "build": "ng build --prod && npm ci --prefix netlify && npm run lambda:tsc && npm run nlb && ./cleanLambdas.sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && netlify deploy",
    "nlb": "netlify functions:build --src netlify/lambdas",
    "lambda:tsc": "npm run build --prefix netlify",
    "lambda:tsc:watch": "npm start --prefix netlify",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0-next.15",
    "@angular/cdk": "^12.2.9",
    "@angular/common": "~13.0.0-next.15",
    "@angular/compiler": "~13.0.0-next.15",
    "@angular/core": "~12.2.9",
    "@angular/flex-layout": "^9.0.0-beta.27",
    "@angular/forms": "~13.0.0-next.15",
    "@angular/material": "^12.2.9",
    "@angular/platform-browser": "~13.0.0-next.15",
    "@angular/platform-browser-dynamic": "~13.0.0-next.15",
    "@angular/router": "~13.0.0-next.15",
    "@angular/service-worker": "~13.0.0-next.15",
    "netlify-cli": "^2.40.0",
    "rxjs": "~6.5.5",
    "tslib": "^1.11.1",
    "zone.js": "~0.11.4",
    "@angular/cli": "~12.2.9",
    "@angular-devkit/build-angular": "~12.2.9",
    "@angular/compiler-cli": "~13.0.0-next.15",
    "@angular/language-service": "~13.0.0-next.15",
    "typescript": "~4.3.5",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.3"
  },
  "devDependencies": {}
}