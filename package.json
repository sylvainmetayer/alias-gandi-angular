{
  "name": "alias-gandi-angular",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload --proxy-config proxy.config.json",
    "prod": "ng serve --live-reload --prod --aot --proxy-config proxy.config.json",
    "build": "ng build --prod --aot && npm run build:lambda",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run postinstall && npm run build && netlify deploy",
    "nls": "npm run postinstall && netlify dev -f netlify/lambdas/",
    "build:lambda": "npm run postinstall && netlify functions:build --src netlify/lambdas",
    "postinstall": "netlify-lambda install netlify/lambdas"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.4",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~9.0.4",
    "@angular/compiler": "~9.0.4",
    "@angular/core": "~9.0.4",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~9.0.4",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~9.0.4",
    "@angular/platform-browser-dynamic": "~9.0.4",
    "@angular/router": "~9.0.4",
    "@angular/service-worker": "~9.0.4",
    "hammerjs": "^2.0.8",
    "netlify-cli": "^2.27.0",
    "netlify-lambda": "^1.6.3",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular/cli": "~9.0.4",
    "@angular/compiler-cli": "~9.0.4",
    "@angular/language-service": "~9.0.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.7.5"
  }
}
